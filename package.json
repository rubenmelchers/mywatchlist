{
  "name": "mywatchlist",
  "version": "1.0.0",
  "description": "mywatchlist",
  "main": "main.js",
  "scripts": {
    "babel:build:js": "babel src --out-file dist/bundle.js",
    "babel:watch:js": "babel src --watch --out-file dist/bundle.js",
    "build:css": "node-sass --include-path scss scss/main.scss css/style.css",
    "watch:css": "nodemon -e scss -x \" npm run build:css \"",
    "webpack:build:js": "webpack",
    "webpack:watch": "webpack --watch --progress --colors"
  },
  "author": "moora@hr.nl",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "backbone": "^1.3.3",
    "css-loader": "^0.28.3",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "underscore": "^1.8.3",
    "webpack": "^2.0.0"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "jquery": "^3.2.1",
    "underscore": "^1.8.3"
  }
}
